/var/log/squid/*.log {
    weekly
    rotate 5
    copytruncate
    compress
    notifempty
    missingok

# This script asks squid to rotate its logs on its own.
# Restarting squid is a long process and it is not worth
# doing it just to rotate logs
    postrotate
      /usr/sbin/squid -k rotate
    endscript
}
